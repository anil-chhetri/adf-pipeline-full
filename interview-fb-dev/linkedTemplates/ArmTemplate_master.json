{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "interview-fb-dev"
		},
		"LS_DataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_DataLake'"
		},
		"LS_AzureDatabricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_AzureDatabricks'"
		},
		"LS_SQL_database_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_database'"
		},
		"LS_DataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datastoresfd.dfs.core.windows.net/"
		},
		"LS_webdata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://restcountries.com/v3.1/region/europe"
		},
		"LS_AzureDatabricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0127-134427-xdty1m2"
		},
		"default_properties_Environment_value": {
			"type": "string",
			"defaultValue": "dev"
		},
		"default_properties_StorageAccount_value": {
			"type": "string",
			"defaultValue": "datastoresfd"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "interview-fb-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_DataLake_accountKey": {
						"value": "[parameters('LS_DataLake_accountKey')]"
					},
					"LS_AzureDatabricks_accessToken": {
						"value": "[parameters('LS_AzureDatabricks_accessToken')]"
					},
					"LS_SQL_database_connectionString": {
						"value": "[parameters('LS_SQL_database_connectionString')]"
					},
					"default_properties_Environment_value": {
						"value": "[parameters('default_properties_Environment_value')]"
					},
					"default_properties_StorageAccount_value": {
						"value": "[parameters('default_properties_StorageAccount_value')]"
					},
					"LS_DataLake_properties_typeProperties_url": {
						"value": "[parameters('LS_DataLake_properties_typeProperties_url')]"
					},
					"LS_webdata_properties_typeProperties_url": {
						"value": "[parameters('LS_webdata_properties_typeProperties_url')]"
					},
					"LS_AzureDatabricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_AzureDatabricks_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}