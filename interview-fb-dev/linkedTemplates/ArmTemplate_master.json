{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "interview-fb-dev"
		},
		"LS_DataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_DataLake'"
		},
		"LS_bronzeLayer_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_bronzeLayer'"
		},
		"LS_inputFolder_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_inputFolder'"
		},
		"LS_inputLayer_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_inputLayer'"
		},
		"LS_DataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datastoresfd.dfs.core.windows.net/"
		},
		"LS_bronzeLayer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datastoresfd.dfs.core.windows.net/"
		},
		"LS_inputFolder_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datastoresfd.dfs.core.windows.net/"
		},
		"LS_inputLayer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datastoresfd.dfs.core.windows.net/"
		},
		"LS_webdata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://restcountries.com/v3.1/region/europe"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "interview-fb-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_DataLake_accountKey": {
						"value": "[parameters('LS_DataLake_accountKey')]"
					},
					"LS_bronzeLayer_accountKey": {
						"value": "[parameters('LS_bronzeLayer_accountKey')]"
					},
					"LS_inputFolder_accountKey": {
						"value": "[parameters('LS_inputFolder_accountKey')]"
					},
					"LS_inputLayer_accountKey": {
						"value": "[parameters('LS_inputLayer_accountKey')]"
					},
					"LS_DataLake_properties_typeProperties_url": {
						"value": "[parameters('LS_DataLake_properties_typeProperties_url')]"
					},
					"LS_bronzeLayer_properties_typeProperties_url": {
						"value": "[parameters('LS_bronzeLayer_properties_typeProperties_url')]"
					},
					"LS_inputFolder_properties_typeProperties_url": {
						"value": "[parameters('LS_inputFolder_properties_typeProperties_url')]"
					},
					"LS_inputLayer_properties_typeProperties_url": {
						"value": "[parameters('LS_inputLayer_properties_typeProperties_url')]"
					},
					"LS_webdata_properties_typeProperties_url": {
						"value": "[parameters('LS_webdata_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}